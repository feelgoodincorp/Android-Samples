<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">

    <aapt:attr name="android:drawable">
        <vector
            android:width="100dp"
            android:height="100dp"
            android:viewportWidth="100.0"
            android:viewportHeight="100.0">

            <group android:name="line1"
                android:pivotX="50.0"
                android:pivotY="50.0"
                android:rotation="0.0">
                <group android:name="line1_2"
                    android:pivotX="50.0"
                    android:pivotY="50.0"
                    android:rotation="0.0">
                    <group android:name="line1_3"
                        android:pivotX="71.0"
                        android:pivotY="29.0"
                        android:rotation="0.0">
                    <path
                        android:name="line"
                        android:fillColor="#000000"
                        android:strokeColor="#000000"
                        android:strokeWidth="5"
                        android:pathData="
                                M20,28
                                L80,28
                                A1,1 0 0,1 80,30
                                L20,30
                                A1,1 0 0,1 20,28"/>
                    </group>
                </group>
            </group>


            <group android:name="line2"
                android:pivotX="50.0"
                android:pivotY="50.0">
                <group android:name="line2_2"
                    android:pivotX="50.0"
                    android:pivotY="50.0">
                    <group android:name="line2_3"
                        android:pivotX="50.0"
                        android:pivotY="50.0"
                        android:rotation="0.0">
                        <path
                            android:name="line2"
                            android:fillColor="#000000"
                            android:strokeColor="#000000"
                            android:strokeWidth="5"
                            android:pathData="
                                M20,49
                                L80,49
                                A1,1 0 0,1 80,51
                                L20,51
                                A1,1 1 1,1 20,49"/>
                    </group>
                </group>
            </group>

            <group android:name="line3"
                android:pivotX="50.0"
                android:pivotY="50.0"
                android:rotation="0.0">
                <group android:name="line3_2"
                    android:pivotX="50.0"
                    android:pivotY="50.0"
                    android:rotation="0.0">
                    <group android:name="line3_3"
                        android:pivotX="29.0"
                        android:pivotY="71.0"
                        android:rotation="0.0">
                        <path
                            android:name="line3"
                            android:fillColor="#000000"
                            android:strokeColor="#000000"
                            android:strokeWidth="5"
                            android:pathData="
                                M20,70
                                L80,70
                                A1,1 0 0,1 80,72
                                L20,72
                                A1,1 1 1,1 20,70"/>
                    </group>
                </group>
            </group>


        </vector>
    </aapt:attr>



    <target android:name="line1_3">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android"
                android:ordering="together">
                <objectAnimator
                    android:duration="@integer/duration"
                    android:interpolator="@android:anim/accelerate_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="-45"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="line3_3">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android"
                android:ordering="together">
                <objectAnimator
                    android:duration="@integer/duration"
                    android:interpolator="@android:anim/accelerate_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="-45"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="line2_2">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android"
                android:interpolator="@android:anim/decelerate_interpolator"
                android:ordering="together"
                android:shareInterpolator="false">

                <objectAnimator
                    android:duration="1000"
                    android:interpolator="@android:anim/accelerate_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="-45"/>
                <objectAnimator
                    android:duration="1000"
                    android:startOffset="1000"
                    android:propertyName="rotation"
                    android:valueFrom="-45"
                    android:valueTo="-225"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="500"
                    android:startOffset="1000"
                    android:valueFrom="1.0"
                    android:valueTo="1.291" />
                <objectAnimator
                    android:startOffset="1000"
                    android:duration="500"
                    android:propertyName="scaleX"
                    android:valueFrom="1.0"
                    android:valueTo="1.291" />

            </set>
        </aapt:attr>
    </target>


    <target android:name="line1_2">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android"
                android:ordering="together">

                <objectAnimator
                    android:duration="@integer/duration"
                    android:startOffset="1000"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="-90"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="line3_2">
        <aapt:attr name="android:animation">
            <set xmlns:android="http://schemas.android.com/apk/res/android"
                android:ordering="together">

                <objectAnimator
                    android:duration="@integer/duration"
                    android:startOffset="1000"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="-90"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>